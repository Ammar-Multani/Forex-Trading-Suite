{
  "expo": {
    "owner": "Forex Trading Suite",
    "name": "forex-trading-suite",
    "slug": "user-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.appacella.userapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appacella-logo-white.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://project-586037e8.dev.kiki.dev"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourceslnReleaseBuilds": true,
            "useLegacyPackaging": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
