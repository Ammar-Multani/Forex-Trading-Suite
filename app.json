{
  "expo": {
    "name": "FX Trading Suite",
    "slug": "fx-trading-suite",
    "scheme": "fxsuite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/fonts/*",
      "assets/*.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enacton.fxsuite",
      "buildNumber": "1.0.0"
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.enacton.fxsuite",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-light.png",
        "monochromeImage": "./assets/adaptive-icon-dark.png",
        "backgroundColor": "#ffffff"
      },
      "allowBackup": false,
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ecedee",
          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#151718"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eb5e48f9-da26-49ca-a539-cbb68d50d45e"
      }
    },
    "owner": "ammar001",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
